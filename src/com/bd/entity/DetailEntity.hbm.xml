<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bd.entity.DetailEntity" table="detail" schema="blog" lazy="false">
        <id name="id">
            <column name="id" sql-type="int(10)"/>
            <generator class="native"/>
        </id>
        <property name="downloadTime">
            <column name="downloadTime" sql-type="date" not-null="false"/>
        </property>
        <many-to-one name="user" class="com.bd.entity.UserEntity">
            <column name="uId" not-null="false"/>
        </many-to-one>
        <many-to-one name="resource" class="com.bd.entity.ResourceEntity">
            <column name="rId" not-null="false"/>
        </many-to-one>
    </class>
</hibernate-mapping>