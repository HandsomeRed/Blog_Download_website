<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bd.entity.FavoriteResourceEntity" table="favorite_resource" schema="blog" lazy="false">
        <id name="id">
            <column name="id" sql-type="int(10)"/>
            <generator class="native"/>
        </id>
        <many-to-one name="resourceMng" class="com.bd.entity.ResourceMngEntity">
            <column name="rmId" not-null="false"/>
        </many-to-one>
        <many-to-one name="resource" class="com.bd.entity.ResourceEntity">
            <column name="rId" not-null="false"/>
        </many-to-one>
    </class>
</hibernate-mapping>